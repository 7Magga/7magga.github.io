                                                    <div class="form-group">
                                                        <input asp-for="UserName" class="form-control" placeholder="Nome de usuario" />
                                                        <input asp-for="Senha" type="password" class="form-control" placeholder="Nova Senha" />
                                                        <input asp-for="ChaveSeguranca" class="form-control" placeholder="Chave de segurança" />
                                                    </div>
                                                    <div class="form-group mx-sm-3">
                                                    </div>
                                                    <button type="submit" class="btn btn-primary">Resetar</button>
                                                </form>
                                                @if (!string.IsNullOrEmpty(@ViewBag.Sucess))
                                                {
                                                <div class="alert alert-success alerta" role="alert" style="color:white">
                                                    @ViewBag.Sucess.ToString()
                                                </div>
                                                }
                                                @if (!string.IsNullOrEmpty(@ViewBag.Erro))
                                                {
                                                <div class="alert alert-danger alerta" role="alert" style="color:white">
                                                    @ViewBag.Erro.ToString()
                                                </div>
                                                }
                                            </div>
                                        </li>
                                        <li class="list-group-item border-0 px-0">
