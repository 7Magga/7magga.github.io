                                                <div class="form-group">
                                                  <input asp-for="Nome" class="form-control" placeholder="Nome" />
                                                  <span asp-validation-for="Nome" class="text-danger"></span>
                                                  <input asp-for="UserName" class="form-control" placeholder="UserName"/>
                                                  <span asp-validation-for="UserName" class="text-danger"></span>
                                                  <input asp-for="Senha" class="form-control" placeholder="Senha"/>
                                                  <span asp-validation-for="Senha" class="text-danger"></span>
                                                  <input asp-for="Cargo" class="form-control" placeholder="Cargo"/>
                                                  <span asp-validation-for="Cargo" class="text-danger"></span>
                                                  <input asp-for="Grupo" class="form-control" placeholder="Grupo"/>
                                                  <span asp-validation-for="Grupo" class="text-danger"></span>
                                                  <input asp-for="ChaveSeguranca" class="form-control" placeholder="Chave de Seguranca"/>
                                                  <span asp-validation-for="ChaveSeguranca" class="text-danger"></span>
                                                </div>
                                                <button type="submit" class="btn btn-primary">Cadastrar</button>
                                            </form>
